<h3> ALERT VIEW </h3>
<div class="container">
   <table class="table table-hover table-hover table-responsive">
      <thead>
         <tr>
            <th>User ID</th>
            <th>First Name</th>
            <th>Middle Name</th>
            <th>Last Name</th>
            <th style='width:10%'>Date of Birth</th>
            <th>Nationality</th>
            <th>Country</th>
            <th style='width:10%'>Date Created</th>            
            <th>City</th>
            <th style='width:20%'>Rules Broken</th>
            <th style='width:20%'>Status</th>            
         </tr>
      </thead>
      <tbody>
         {{#each users}}
         <tr>
            <td><a href="http://localhost:3000/userid?id={{user_id}}" target="_blank">{{user_id}} </a>  </td>
            <td> {{first_name}}</td>
            <td> {{middle_name}}</td>
            <td> {{last_name}}</td>
            <td> {{moment date_of_birth "DD / MM / YYYY"}}</td>
            <td> {{nationality}}</td>
            <td> {{country_of_residence}}</td>
            <td> {{moment created_at "DD / MM / YYYY"}}</td>            
            <td> {{city}}</td> 
            <td> <a href="http://localhost:3000/singletransactionqyeryresult?id={{ID}}" target="_blank">{{RULE}}</a></td> 
            {{#equal user_id 15}} 
            <td bgcolor="#008000"></td> 
            {{/equal}}
			{{#notequal user_id 15}} 
            <td bgcolor="#008090"></td> 
            {{/notequal}}
            			
         </tr>
         {{/each}}
      </tbody>
   </table>
</div>
<h3>CUSTOMER VIEW KYC Information</h3>
<div class="row">
   <div class="col-xs-6">
      <p>
         <b>PERSONAL</b><br>
         {{#each users1}}		  
         FIRST NAME :- {{first_name}} <br>
         LAST NAME :- {{last_name}} <br>
         DATE OF BIRTH :- {{moment date_of_birth "DD / MM / YYYY"}} <br>  
         NATIONALITY :- {{nationality}} <br>
         GENDER :- {{gender}} <br>
         INDUSTRY :- {{industry}} <br>
         OCCUPATION :- {{work_type}} <br>
         TAX ID :- {{tax_id}} <br>
         {{/each}}
      </p>
   </div>
   <div class="col-xs-6">
      <p><b>ADDRESS</b><br>
         {{#each users1}}
         Address 1 :- {{address_line_1}} <br>
         Address 2 :- {{address_line_2}} <br>
         City :- {{city}} <br>
         State :- {{state}} <br>
         Postal Code :- {{postal_code}} <br>
         Country :- {{country_of_residence}} <br>
         Contact number :- {{contact_number}} <br>
         Email Address :- {{img_selfie}} <br>
         Utility Bill :- {{img_selfie}} <br><br>
         {{/each}}
      </p>
   </div>
</div>
<div class="row">
   <div class="col-xs-6">
      <p>
         <b>DOCUMENT</b><br>
         {{#each users1}}
         ID TYPE :- {{document_type}} <br>
         ID NUMBER :- {{document_number}} <br>
         ID EXPIRY :- {{document_expiry}} <br>
         ID FRONT URL :- {{ img_front }} <br>
         SELFIE URL :- {{img_selfie}} <br>
         {{/each}}
      </p>
   </div>
   <div class="col-xs-6">
      <p>
         <b>FINANCE</b><br>
         {{#each users1}}
         Purpose of Action :- {{purpose_of_action}} <br>
         Planned Investment Amount :- {{planned_investment}} <br>
         {{/each}}
      </p>
   </div>
</div>
